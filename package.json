{
  "name": "@shravya-kasturi/add",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist/**.*"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc && rollup -c rollup.config.js",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "tag": "latest",
    "@shravya-kasturi:registry": "https://npm.pkg.github.com"
  },
  "release": {
    "getLastRelease": "last-release-git",
    "branches": [
      "main",
      "next",
      "feature/**"
    ],
    "debug": true,
    "repositoryUrl": "git://github.com/shravya-kasturi/add.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      ["@semantic-release/npm", {
        "pkgRoot": "dist",
        "npmPublish": false,
        "tarballDir": "dist"
      }],
      ["@semantic-release/github", {
        "assets": "dist/*.tgz"
      }]
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/shravya-kasturi/add.git",
    "directory": "dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/jest": "^26.0.24",
    "eslint": "^7.29.0",
    "jest": "^27.0.6",
    "last-release-git": "0.0.3",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "semantic-release": "^17.4.4",
    "ts-jest": "^27.0.3",
    "typescript": "^3.9.10"
  }
}
